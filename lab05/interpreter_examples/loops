int a = 0;

while (a < 5) {
      print a;
      a = a + 1;
}

print a;

repeat {
       a = a - 1;
} until a < 6;

print a;

print "Now breaks";
a = 0;
while (a < 10) {
      if (a == 3) break;
      print a;
      a = a + 1;
}
print "BROKEN";

print "With repeat";
repeat {
       a = a - 1;
       if (a == 1) break;
} until (a == (-4));
print a;

print "Continue";
a = 0;
while (a < 10) {
      a = a + 1;
      if (a == 8) continue;
      print a;
}